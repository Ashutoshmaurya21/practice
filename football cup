#include <iostream>
using namespace std;

int main() {
	int n,m,i,j;
	cin>>n>>m;
	int arr[n][m];
	for(int i=0;i<n;i++){
	    for(int j=0;j<m;j++){
	    cin>>arr[i][j];
	    }
	    cout<<" ";}
	    cout<<"matrix of the :";
	    	for(int i=0;i<n;i++){
	   
	    	     for(int j=0;j<m;j++){
	    cout<<arr[i ][j];}
	    cout<<endl;
	    
	    if(i==j&& i!=0&&j!=0){
	        cout<<"YES";}
	        else if(i!=j){
	            cout<<"NO";
	        }
	    
	    
	    	}	
	return 0;
}
